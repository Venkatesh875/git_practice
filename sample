#include <stdio.h>
int main() {    

    int number1, number2, sum,sub;
    int number1, number2, sum,sub,mul;
    printf("Enter two integers: ");
    scanf("%d %d", &number1, &number2);

    // calculate the sum
    sum = number1 + number2;      
    sub = number1 - number2;
    mul=number1 * number2;
    printf("%d + %d = %d", number1, number2, sum);
    return 0;
}
